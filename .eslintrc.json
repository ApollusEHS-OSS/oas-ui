{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier",
    "react",
    "react-hooks"
  ],
  "rules": {
    "no-duplicate-imports": 2,
    "no-unused-vars": "error",
    "prefer-const": 1,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}
